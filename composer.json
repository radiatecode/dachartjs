{
    "name": "radiatecode/dachart",
    "description": "laravel package to generate chart using the chart.js",
    "license": "MIT",
    "authors": [
        {
            "name": "radiatecode",
            "email": "radiate126@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1|^8.0",
        "ext-json": "*",
        "illuminate/support": "^8.73"
    },
    "require-dev": {
        "orchestra/testbench": "^6.18",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "RadiateCode\\DaChart\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RadiateCode\\DaChart\\tests\\" : "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers" : [
                "RadiateCode\\DaChart\\ChartServiceProvider"
            ]
        }
    }
}
